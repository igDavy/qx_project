<svg xmlns="http://www.w3.org/2000/svg" width="1200" height="600" viewBox="0 0 1200 600">
  <defs>
    <linearGradient id="bg-gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#43a047" stop-opacity="0.8"/>
      <stop offset="100%" stop-color="#2e7d32" stop-opacity="0.9"/>
    </linearGradient>
  </defs>
  <rect width="1200" height="600" fill="url(#bg-gradient)"/>
  
  <!-- 发动机图形 -->
  <g transform="translate(200, 150)">
    <rect x="0" y="0" width="300" height="300" rx="20" fill="#ffffff" fill-opacity="0.15"/>
    
    <!-- 发动机内部结构 -->
    <circle cx="150" cy="150" r="100" fill="none" stroke="#ffffff" stroke-width="5"/>
    <circle cx="150" cy="150" r="80" fill="none" stroke="#ffffff" stroke-width="5"/>
    <circle cx="150" cy="150" r="60" fill="none" stroke="#ffffff" stroke-width="5"/>
    
    <line x1="50" y1="150" x2="250" y2="150" stroke="#ffffff" stroke-width="5"/>
    <line x1="150" y1="50" x2="150" y2="250" stroke="#ffffff" stroke-width="5"/>
    
    <!-- 齿轮 -->
    <g transform="translate(250, 250)">
      <circle cx="0" cy="0" r="40" fill="#ffffff" fill-opacity="0.2"/>
      <circle cx="0" cy="0" r="20" fill="#ffffff" fill-opacity="0.3"/>
      <path d="M0,-40 L5,-30 L-5,-30 Z" fill="#ffffff" transform="rotate(0)"/>
      <path d="M0,-40 L5,-30 L-5,-30 Z" fill="#ffffff" transform="rotate(45)"/>
      <path d="M0,-40 L5,-30 L-5,-30 Z" fill="#ffffff" transform="rotate(90)"/>
      <path d="M0,-40 L5,-30 L-5,-30 Z" fill="#ffffff" transform="rotate(135)"/>
      <path d="M0,-40 L5,-30 L-5,-30 Z" fill="#ffffff" transform="rotate(180)"/>
      <path d="M0,-40 L5,-30 L-5,-30 Z" fill="#ffffff" transform="rotate(225)"/>
      <path d="M0,-40 L5,-30 L-5,-30 Z" fill="#ffffff" transform="rotate(270)"/>
      <path d="M0,-40 L5,-30 L-5,-30 Z" fill="#ffffff" transform="rotate(315)"/>
    </g>
  </g>
  
  <!-- 工具图标 -->
  <g transform="translate(700, 200)">
    <path d="M50,50 L250,50 L250,250 L50,250 Z" fill="none" stroke="#ffffff" stroke-width="5"/>
    <path d="M70,100 L230,100" stroke="#ffffff" stroke-width="5"/>
    <path d="M70,150 L230,150" stroke="#ffffff" stroke-width="5"/>
    <path d="M70,200 L230,200" stroke="#ffffff" stroke-width="5"/>
    
    <path d="M100,70 L100,230" stroke="#ffffff" stroke-width="5"/>
    <path d="M150,70 L150,230" stroke="#ffffff" stroke-width="5"/>
    <path d="M200,70 L200,230" stroke="#ffffff" stroke-width="5"/>
  </g>
  
  <!-- 文字 -->
  <text x="600" y="150" font-family="Arial, sans-serif" font-size="60" font-weight="bold" fill="#ffffff" text-anchor="middle">专业汽车维修服务</text>
  <text x="600" y="220" font-family="Arial, sans-serif" font-size="30" fill="#ffffff" text-anchor="middle">发动机维修、电路检测、底盘保养</text>
</svg>